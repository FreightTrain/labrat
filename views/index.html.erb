<!doctype html>
<html>
  <head>
    <title>
      Environment information
    </title>
  </head>

  <body>
    <article>
      <table>
        <% ENV.each do |k, v| %>
          <tr>
            <td><%= k %></td>
            <td><%= v.inspect %></td>
          </tr>
        <% end %>
      </table>
    </article>
    <article>
      <p>
        Parsed VCAP_SERVICES: <%= MultiJson.load(ENV.fetch("VCAP_SERVICES", "{}")).inspect %>
      </p>
    </article>
  </body>
</html>
